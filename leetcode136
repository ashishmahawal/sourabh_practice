nums = [4,1,2,1,2]
res =[]
for i in range(0,len(nums)):
    for j in range(i+1,len(nums)):
        if nums[i] != nums[j]:
            res = nums[i]
        elif nums[j] != nums[i]:
            res = nums[j]
print(res)

# another way 
numlist =[1,2,1,2,4]
for num in numlist:
    if numlist.count(num)==1:
        print(num)